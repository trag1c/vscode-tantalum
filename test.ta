is_prime(n) {
    n <= 3 -> n > 1.
    n % 2 == 0 | n % 3 == 0 -> false.
    i := 5
    i * i <= n <-> {
        n % i == 0 | n % (i + 2) == 0 -> false.
        i += 6
    }
    true.
}

main() {
    println("Primes under 1000:")
    1000 >> i >> {
        is_prime(i) -> print("{} " << i)
    }
    println()
}